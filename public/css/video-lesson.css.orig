/* BOTTOM USERS STYLES */
.small-participant-track-controls {
  position: absolute;
  top: 0;
  left: 0;
  display: flex;
  justify-content: space-between;
  width: 200px;
  padding: 0 12px;
  box-sizing: border-box;
  opacity: 0;
  pointer-events: none; }

.remote-participant-track:hover .small-participant-track-controls {
  opacity: 1;
  pointer-events: auto; }

.remote-participant-track:hover .small-participant-track-controls {
  opacity: 1;
  pointer-events: auto; }

.small-participant-track-controls button {
  background-color: transparent;
  border: none; }

/* BOTTOM USERS STYLES END */
.participantsTracks-inner .participant-name,
.remote-participant-track .participant-name {
  color: #fff;
  font-family: "Poppins";
  font-size: 14px;
  line-height: 24px;
  opacity: 0;
  bottom: 20px;
  position: absolute;
  left: 20px; }

.videos--wrapper {
  display: flex;
  justify-content: center;
  align-items: flex-end;
  position: relative; }

.expanded-mode .videos--wrapper #instructor-track-container {

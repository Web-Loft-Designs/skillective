#chat-messages-container ul li img {
  width: 54px;
  height: 54px;
  -o-object-fit: contain;
     object-fit: contain;
  border-radius: 50%;
  flex-shrink: 0;
  margin-left: 14px; }

.chat-controls {
  width: 100%; }

.chat-controls label {
  color: #444;
  margin-bottom: 5px;
  font-family: "Hind Vadodara";
  font-size: 14px; }

.chat-controls textarea {
